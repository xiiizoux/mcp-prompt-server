{
  "name": "mcp-prompt-server",
  "version": "1.0.0",
  "description": "MCP服务器，提供预设的prompt工具来引导Claude执行任务",
  "main": "dist/src/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "setup": "npm install",
    "lint": "eslint . --ext .ts",
    "vercel-build": "tsc",
    "dev:api": "vercel dev",
    "server": "node server.js",
    "use:file": "node scripts/switch-storage.js file",
    "use:supabase": "node scripts/switch-storage.js supabase"
  },
  "keywords": [
    "mcp",
    "claude",
    "prompt",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@supabase/supabase-js": "^2.49.8",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.21",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "vercel": "^33.6.1"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
