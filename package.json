{
  "name": "mcp-prompt-server",
  "version": "1.0.0",
  "description": "MCP服务器，提供预设的prompt工具来引导Claude执行任务",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/server.js",
    "start:ts": "ts-node server.ts",
    "dev": "ts-node-dev --respawn server.ts",
    "build": "tsc",
    "setup": "npm install",
    "lint": "eslint . --ext .ts",
    "prod": "npm run build && npm start"
  },
  "keywords": [
    "mcp",
    "claude",
    "prompt",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@supabase/supabase-js": "^2.49.8",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.21",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
